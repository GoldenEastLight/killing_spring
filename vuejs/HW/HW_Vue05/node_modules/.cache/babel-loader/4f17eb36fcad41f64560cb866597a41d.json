 '',\r\n      items: [],\r\n    };\r\n  },\r\n  created() {\r\n    /*\r\n    http\r\n      .get('/board')\r\n      .then(({ data }) => {\r\n        this.items = data;\r\n      })\r\n      .catch(() => {\r\n        alert('에러가 발생했습니다.');\r\n      });\r\n      */\r\n    this.getBoard();\r\n  },\r\n  methods: {\r\n    async getBoard() {\r\n      try {\r\n        let { data } = await http.get('/employee/all');\r\n        this.items = data;\r\n      } catch (error) {\r\n        alert('에러가 발생했습니다.');\r\n      }\r\n    },\r\n    movePage() {\r\n      this.$router.push('/create');\r\n    },\r\n    searchBoard() {\r\n      http.get('/board').then(({ data }) => {\r\n        const boards = data;\r\n        let newItems = {\r\n          items: [],\r\n        };\r\n        boards.forEach((board) => {\r\n          if (board.title.indexOf(this.search) !== -1) {\r\n            newItems.items.push({\r\n              key: board.key,\r\n              no: board.no,\r\n              title: board.title,\r\n              writer: board.writer,\r\n              regtime: board.regtime,\r\n            });\r\n          }\r\n        });\r\n        console.dir(newItems);\r\n        this.items = newItems.items;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.text-right {\r\n  text-align: right !important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/page"}]}